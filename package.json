{
  "name": "the-tap",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ls-build": "lsc -cbp webpack.config.ls > webpack.config.js && lsc -cbo material material && lsc -cbo build build",
    "webpack-dev": "npm run ls-build && webpack --display-error-details",
    "webpack-prod": "npm run ls-build && webpack --display-error-details -p",
    "serve": "http-server . -c-1 -o -p 8888",
    "all-dev": "npm run pre-clean && npm run webpack-dev && npm run post-clean && npm run serve",
    "all-prod": "npm run pre-clean && npm run webpack-prod && npm run post-clean && npm run serve",
    "pre-clean": "rimraf *.js *.html",
    "post-clean": "rimraf material/*.js material/*/**.js build/*.js webpack.config.js"
  },
  "author": "",
  "license": "Zlib",
  "dependencies": {
    "autoload": "^0.1.2",
    "lunr": "^2.1.6",
    "pixi-sound": "^2.0.2",
    "pixi.js": "^4.7.3"
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "css-loader": "^0.28.11",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "livescript": "^1.5.0",
    "livescript-loader": "^0.1.6",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14"
  }
}
